        if (idpLogin == "false") {
            swal({ type: 'info', title: "Information", text: "Login with UAEID to access this module" });
            return;
        }

        $.ajax({
            url: '@Url.Action("DocumentIssuerPrivilage", "DocumentIsuuer")',
            type: 'GET',
            dataType: 'json',
            success: function (response) {
                if (response.status === "Success" && response.result.organisationPrivileges.documentIssuer === true) {
                    window.location.href = '@Url.Action("List", "DocumentIsuuer")' // Redirect if issuer is true
                } else {
                    swal({ type: 'error', title: "Access Denied", text: "You don't have permission to access this module." });
                }
            },
            error: function () {
                swal({ type: 'error', title: "Error", text: "Something went wrong, please try again later." });
            }
        });
    }
