		// document.getElementById("primaryImage").src = "data:image/jpg;base64," + img1
		// document.getElementById("secondaryImage").src = "data:image/jpg;base64," + imgData
		// $("#primaryImage, #secondaryImage").click(function () {
		//     window.location.href = '@Url.Action("Index", "Dashboard")'
		// });
		idpLogin = '@IDPLogin';
		bulkSign = '@bulkSign';
		spoc_Email = '@spoc_email';
		user_Email = '@user_email';
		prePaidOrPostPaid = '@prePaidOrPostPaid';
		checkDelegationPrevillage = '@DelegateUser';
	});

	document.addEventListener("DOMContentLoaded", function () {
		if (window.innerWidth >= 992) {
			const dropdownToggles = document.querySelectorAll('.navbar .dropdown-toggle');
			dropdownToggles.forEach(toggle => {
				toggle.addEventListener('click', function (e) {
					e.preventDefault();
				});
			});
		}

		const layout = getCookie("HeaderNavigationlLayout");
		const isVertical = (layout === "Vertical");

		// Set toggle state
		const toggleSwitch = document.getElementById('navLayoutToggle');
		if (toggleSwitch) {
			toggleSwitch.checked = isVertical;
		}

		// Apply the layout visually
		applyNavLayout(isVertical);
	});


	function toggleNavLayout() {
		const isVertical = document.getElementById('navLayoutToggle').checked;
		const layout = isVertical ? 'Vertical' : 'Horizontal';
		document.cookie = "HeaderNavigationlLayout=" + layout + "; path=/";
		applyNavLayout(isVertical);
	}

	function customChange(ltype) {
		document.cookie = "HeaderNavigationlLayout=" + ltype + "; path=/";
		applyNavLayout();
	}

	function applyNavLayout(isVertical) {
		const sidebar = document.getElementsByClassName("nsidebar");
		const mainHorizontalNav = document.getElementById("mainHorizontalNav");
		const hlogo = document.getElementById("primary-image");
		const profile = document.getElementById("navbarNavDropdown");
		const hicon = document.getElementById("sidebarCollapse");

		if (hicon) {
			hicon.style.display = isVertical ? "" : "none";
		}

		if (profile) {
			profile.style.justifyContent = isVertical ? "flex-end" : "space-between";
		}

		for (let i = 0; i < sidebar.length; i++) {
			sidebar[i].style.display = isVertical ? "block" : "none";
		}

		if (mainHorizontalNav) {
			mainHorizontalNav.style.display = isVertical ? "none" : "flex";
			mainHorizontalNav.style.justifyContent = isVertical ? "flex-end" : "";
			hlogo.style.display = isVertical ? "none" : "block";
		}
	}


	function getCookie(name) {
		const value = "; " + document.cookie;
		const parts = value.split("; " + name + "=");
		if (parts.length === 2) return parts.pop().split(";").shift();
	}

	document.addEventListener("DOMContentLoaded", function () {
		const submenuItems = document.querySelectorAll(".dropdown-item");

		submenuItems.forEach(item => {
			const itemHref = item.href.replace(window.location.origin, '').split('?')[0];

			const currentPath = window.location.pathname;

			if (currentPath.startsWith(itemHref)) {
				const parentKey = item.getAttribute("data-parent");
				const parentNavItem = document.querySelector(`[data-section="${parentKey}"] .nav-link`);
				if (parentNavItem) {
					parentNavItem.classList.add("active");
				}
			}
		});
	});

	function ChangeLayout(layoutType) {
		$.ajax({
			url: '@Url.Action("SetLayout", "Home")',
			type: 'POST',
			data: { Layout: layoutType },
			success: function (response) {
				//window.location.reload();
				window.location.href = response.redirectUrl;
